{
    "name": "cw/ext-bookdatabase",
    "type": "typo3-cms-extension",
    "description": "Simple Bookdatabase",
    "authors": [
        {
            "name": "C. Werner",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "Cw\\ExtBookdatabase\\": "Classes"
        }
    },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/extension_builder ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/ext_bookdatabase"
    ]
  }
}